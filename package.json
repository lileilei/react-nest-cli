{
  "name": "react-cli",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build:client": "cross-env NODE_ENV=production vite build",
    "build:server": "cross-env NODE_ENV=production nest build",
    "build": "rimraf client/src/pages/_ewell.tsx && npm run build:client && npm run build:server",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "cross-env NODE_ENV=development nest start  --watch",
    "prod": "cross-env PORT=3000 cross-env NODE_ENV=production   node build/server/main.js",
    "pm2": "cross-env PORT=3000 cross-env NODE_ENV=production   pm2 start build/server/main.js -i max"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
