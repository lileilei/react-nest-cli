    "alova": "^2.6.1",
    "antd": "^5.4.7",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "figlet": "^1.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.15.0"
  },
  "devDependencies": {
